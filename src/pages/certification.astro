---
import "../styles/global.css";
import Header from "../components/header.astro";
import Footer from "../components/footer.astro";
import Certification from "../components/certification/certification.astro";
---

<!doctype html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta name="author" content="Gunasekaran Palani" />

    <title>
      Certifications | Gunasekaran Palani - Microsoft Certified Software
      Engineer
    </title>

    <!-- SEO Meta Tags -->
    <meta
      name="title"
      content="Certifications | Gunasekaran Palani - Microsoft Certified Software Engineer"
    />
    <meta
      name="description"
      content="View certifications of Gunasekaran Palani: Microsoft Certified Azure Developer (AZ-204), Azure AI Engineer (AI-102), Azure Data Scientist (DP-100), C# Corner MVP, and 100 Days Python Bootcamp by Udemy."
    />
    <meta
      name="keywords"
      content="Gunasekaran Palani Certifications, Microsoft Certified Azure Developer AZ-204, Azure AI Engineer AI-102, Azure Data Scientist DP-100, C# Corner MVP, Udemy Python Bootcamp, Microsoft Certifications, Cloud Certifications, Software Engineer Certifications"
    />

    <link rel="canonical" href="https://gunapalani.github.io/certifications" />
    <link rel="icon" type="image/x-icon" href="/favicon.ico?v=1" />

    <meta name="robots" content="index, follow" />

    <!-- Open Graph / Facebook -->
    <meta property="og:type" content="website" />
    <meta property="og:url" content="https://gunapalani.github.io/certifications" />
    <meta
      property="og:title"
      content="Certifications | Gunasekaran Palani - Microsoft Certified Software Engineer"
    />
    <meta
      property="og:description"
      content="Gunasekaran Palani holds certifications including Microsoft Certified Azure Developer (AZ-204), Azure AI Engineer (AI-102), Azure Data Scientist (DP-100), C# Corner MVP, and Udemy Python Bootcamp."
    />
    <meta
      property="og:image"
      content="https://www.c-sharpcorner.com/UploadFile/AuthorImage/a603d720241104092535.jpg"
    />

    <!-- Twitter -->
    <meta property="twitter:card" content="summary_large_image" />
    <meta
      property="twitter:url"
      content="https://gunapalani.github.io/certifications"
    />
    <meta
      property="twitter:title"
      content="Certifications | Gunasekaran Palani - Microsoft Certified Software Engineer"
    />
    <meta
      property="twitter:description"
      content="Explore certifications of Gunasekaran Palani: Microsoft Azure Developer (AZ-204), Azure AI Engineer (AI-102), Azure Data Scientist (DP-100), C# Corner MVP, Udemy Python Bootcamp."
    />
    <meta
      property="twitter:image"
      content="https://www.c-sharpcorner.com/UploadFile/AuthorImage/a603d720241104092535.jpg"
    />

    <!-- AOS CSS -->
    <link rel="stylesheet" href="https://unpkg.com/aos@next/dist/aos.css" />
  </head>

  <body>
    <Header />
    <Certification />
    <Footer />
  </body>
  <script src="https://unpkg.com/aos@next/dist/aos.js" is:inline></script>
  <script is:inline>
    AOS.init();
  </script>
</html>

<style>
  html,
  body {
    margin: 0;
    width: 100%;
    height: 100%;
  }
</style>
